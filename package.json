{
	"name": "keep-moving",
	"displayName": "Keep Moving",
	"description": "Reminds you to stand up and move every 10 minutes with a visual notification",
	"version": "0.0.9",
	"publisher": "Yusupsupriyadicom",
	"icon": "icon.png",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/yusupsupriyadi/keep-moving-extension"
	},
	"engines": {
		"vscode": "^1.80.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "keep-moving.toggle",
				"title": "Keep Moving: Toggle Reminder"
			},
			{
				"command": "keep-moving.showMenu",
				"title": "Keep Moving: Show Menu"
			},
			{
				"command": "keep-moving.snooze",
				"title": "Keep Moving: Snooze Reminder"
			},
			{
				"command": "keep-moving.setInterval",
				"title": "Keep Moving: Set Reminder Time"
			}
		],
		"configuration": {
			"title": "Keep Moving",
		"properties": {
				"keepMoving.reminderInterval": {
					"type": "number",
					"default": 10,
					"description": "Interval waktu pengingat dalam menit"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js",
		"vscode:prepublish": "npm run compile",
		"compile": "echo Compiling...",
		"package": "vsce package",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"@types/glob": "^8.1.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "20.2.5",
		"@types/vscode": "^1.80.0",
		"@vscode/test-electron": "^2.3.2",
		"eslint": "^8.41.0",
		"glob": "^8.1.0",
		"mocha": "^10.2.0",
		"typescript": "^5.1.3",
		"@vscode/vsce": "^2.15.0"
	},
	"keywords": [
		"health",
		"reminder",
		"productivity"
	],
	"galleryBanner": {
		"color": "#303030",
		"theme": "dark"
	}
}
